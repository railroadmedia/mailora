{
  "name": "railroad/mailora",
  "type": "library",
  "description": "Wrapper for Laravel's email functionality that adds HTTP API and front-end-dev-friendly view-creation and use.",
  "keywords": [
    "email",
    "mail",
    "laravel",
    "api",
    "wrapper"
  ],
  "homepage": "https://github.com/railroad/mailora",
  "license": "GPL-3.0-or-later",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.2",
    "doctrine/dbal": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Mailora\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Mailora\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Railroad\\Mailora\\Providers\\MailoraServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/package-versions-deprecated": true
    }
  }
}